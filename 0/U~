FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1 0 0);

boundaryField
{
    inlet
    {
	type 	freestream;
	freestreamValue	uniform (1 0 0);
    }

    outlet
    {
	type 	zeroGradient;	
    }

    top
    {
	type 	freestream;
	freestreamValue	uniform (1 0 0);
    }

    bottom
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    slipsurface
    {
	type 		slip;
    }

    frontandback
    {
	type	empty;
    }
}


// ************************************************************************* //
